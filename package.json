{
  "name": "kobo-sdk",
  "version": "2.4.2",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,ts,tsx,json}\" --config .prettierrc",
    "release-patch": "./release.sh patch",
    "release-minor": "./release.sh minor",
    "release-major": "./release.sh major",
    "dev": "tsc -w",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "jest"
  },
  "dependencies": {
    "@axanc/ts-utils": "3.0.0",
    "@types/form-data": "^2.5.2",
    "@types/promise-retry": "^1.1.6",
    "@types/qs": "^6.9.15",
    "axios": "1.7.9",
    "date-fns": "4.1.0",
    "dotenv": "^16.4.7",
    "form-data": "^4.0.1",
    "promise-retry": "^2.0.1",
    "qs": "^6.12.1",
    "uuid": "^11.0.5",
    "winston": "^3.13.1",
    "xml-js": "^1.6.11",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
